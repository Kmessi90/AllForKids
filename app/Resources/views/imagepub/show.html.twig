{% extends 'dashboard.html.twig' %}

{% block body %}

<div class="title_left">
    <h1 align="center">Imagepub </h1>
</div>


<div class="x_panel" align="auto">
    <table id="datatable" class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ imagePub.id }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ imagePub.description }}</td>
            </tr>
            <tr>
                <th>Imagename</th>
                <td>{{ imagePub.imageName }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if imagePub.updatedAt %}{{ imagePub.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('imagepub_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('imagepub_edit', { 'id': imagePub.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
